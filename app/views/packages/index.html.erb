<h1>Listing packages</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Price</th>
    <th>Category</th>
    <th>Supercategory</th>
    <th>Type</th>
    <th>User</th>
    <th>Provider</th>
    <th>Image file name</th>
    <th>Image content type</th>
    <th>Image file size</th>
    <th>Image update at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @packages.each do |package| %>
  <tr>
    <td><%= package.name %></td>
    <td><%= package.description %></td>
    <td><%= package.price %></td>
    <td><%= package.category %></td>
    <td><%= package.supercategory %></td>
    <td><%= package.type %></td>
    <td><%= package.user %></td>
    <td><%= package.provider %></td>
    <td><%= package.image_file_name %></td>
    <td><%= package.image_content_type %></td>
    <td><%= package.image_file_size %></td>
    <td><%= package.image_update_at %></td>
    <td><%= link_to 'Show', package %></td>
    <td><%= link_to 'Edit', edit_package_path(package) %></td>
    <td><%= link_to 'Destroy', package, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Package', new_package_path %>
