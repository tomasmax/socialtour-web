<h1>Listing likes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Category</th>
    <th>Facebook</th>
    <th>Created time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @likes.each do |like| %>
  <tr>
    <td><%= like.name %></td>
    <td><%= like.category %></td>
    <td><%= like.facebook_id %></td>
    <td><%= like.created_time %></td>
    <td><%= link_to 'Show', like %></td>
    <td><%= link_to 'Edit', edit_like_path(like) %></td>
    <td><%= link_to 'Destroy', like, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Like', new_like_path %>
