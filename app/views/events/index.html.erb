<h1>Listing events</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Picture</th>
    <th>Url</th>
    <th>Category</th>
    <th>Supercategory</th>
    <th>References</th>
    <th>Poi</th>
    <th>Starts at</th>
    <th>Ends at</th>
    <th>Start time</th>
    <th>Price</th>
    <th>Provider</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.name %></td>
    <td><%= event.description %></td>
    <td><%= event.picture %></td>
    <td><%= event.url %></td>
    <td><%= event.category %></td>
    <td><%= event.supercategory %></td>
    <td><%= event.references %></td>
    <td><%= event.poi %></td>
    <td><%= event.starts_at %></td>
    <td><%= event.ends_at %></td>
    <td><%= event.start_time %></td>
    <td><%= event.price %></td>
    <td><%= event.provider %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_event_path %>
